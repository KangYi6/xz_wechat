<view class="box">
    <van-image
        round
        width="150rpx"
        height="150rpx"
        src="{{avatar}}"
        bindtap="tapAvatar"
    />
    <text>{{nickname}}</text>
</view>

<van-cell title="个人信息" icon="location-o" is-link />
<van-cell title="我的收藏" icon="star-o" is-link />
<van-cell title="我的钱包" icon="refund-o" is-link />
<van-cell title="设置" icon="discount" is-link />